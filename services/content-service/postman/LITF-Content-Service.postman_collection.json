          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"file\": \"{{base64_image_data}}\",\n  \"filename\": \"sample-image.jpg\",\n  \"contentType\": \"image/jpeg\",\n  \"folder\": \"content-images\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/media/upload",
              "host": ["{{baseUrl}}"],
              "path": ["media", "upload"]
            }
          }
        },
        {
          "name": "Delete Image",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"url\": \"{{image_url}}\"\n}"
